# Tradovate API Credentials
# Copy this file to .env and fill in your credentials

# Environment (demo or live)
TRADOVATE_ENV=demo

# Your Tradovate credentials
TRADOVATE_USERNAME=your_username_here
TRADOVATE_PASSWORD=your_password_here

# Optional: API Key authentication (leave empty to use username/password)
TRADOVATE_API_KEY=
TRADOVATE_API_SECRET=

# Trading Parameters
RISK_PER_TRADE_MIN=30
RISK_PER_TRADE_MAX=60
PROFIT_TARGET_R=2

# Contract to trade (MES or MNQ)
CONTRACT_SYMBOL=MESM5

# Strategy Settings
STRATEGY=simple_breakout
TIMEFRAME=5m
